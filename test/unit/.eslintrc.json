{
    "env": {
      "mocha": true,
      "es6": true
    },
    "parserOptions": {
    "ecmaVersion": 2019
    },
    "rules": {
      "require-jsdoc": "off",
      "max-len": "off",
      "quote-props": "off",
      "no-new": "off",
      "valid-jsdoc": "off",
      "no-unused-expressions": "off",
      "no-proto": "off"
    }
  }